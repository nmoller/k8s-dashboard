apiVersion: v1
kind: Service
metadata:
  name: {{ service.service_name }}
  namespace: {{ service.service_ns }}
spec:
  selector:
    app: {{ service.service_selector_app }}
  ports:
    - protocol: TCP
      port: {{ service.service_port }}
      targetPort: {{ service.service_targetPort }}
      nodePort: {{ service.service_nodePort }}
  type: NodePort